<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Custom Report Demo" verbose="10">
	<test name = "CustomReportBaseConfigDataLayout" preserve-order = "true">
		<classes>
			<class name="com.trane.display.cases.TestCustomReport">
				<methods>
					<include name="clickReports" />
					<include name="verifyBtnReportsClicked" />
					<include name="clickCustomReport" />
					<include name="ClickEditButton" />
					<include name="verifyCustomReportData" />
					<include name="tearDown" />
				</methods>
			</class>
		</classes>
	</test>
		<test name = "LogSheetBaseConfigDataLayout" preserve-order = "true">
		<classes>
			<class name="com.trane.display.cases.TestLogSheet">
				<methods>
					<include name="clickReports" />
					<include name="verifyBtnReportsClicked" />
					<include name="clickLogSheet" />
					<include name="verifyBtnLogSheetClicked" />
					<include name="verifyLogSheetData" />
					<include name="tearDown" />
				</methods>
			</class>
		</classes>
	</test>
</suite>